
{   
    "canUseAwsMetrics": false,
    "canUseCustomEvents": true,
    "canUseSysdigCapture": true,
    "default": false,
    "description": "{{ team.description }}",
    "entryPoint": {
        "module": "Dashboards"
    },
    "name": "{{ team.name }}",
    "show": "container",
    "theme": "#73A1F7",
    "filter": "{% for namespace in team.namespaces  %}kubernetes.namespace.name in (\"{{ namespace }}\"){% if not loop.last %} and {% endif %}{% endfor %}",
    "userRoles": [
{% for role in team_users_and_roles  %}
      {
        "userId": "{{ role.userID }}",
        "role": "{{ role.role }}"
      }
  {% if not loop.last %}
      ,
  {% endif %}
{% endfor %}     
    ]
}